FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 4.5E-5;

boundaryField
{
    wing
    {
        type            nutUSpaldingWallFunction;
        value           $internalField;
    }
    sym
    {
        type            symmetry;
    }
    inout
    {
        type            calculated;
        value           $internalField;
    }

}


