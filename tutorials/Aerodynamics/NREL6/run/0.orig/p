FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];


internalField   uniform 101000;

boundaryField
{
    blade
    {
        type            zeroGradient;
    }
    "(inlet|outlet|sides)"
    {
        type            outletInlet;
        outletValue     $internalField;
        value           $internalField;
    }
}


