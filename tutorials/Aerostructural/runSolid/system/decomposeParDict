FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      decomposeParDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

numberOfSubdomains     4;

method             scotch;

simpleCoeffs 
{ 
    n             (2 1 1); 
    delta         0.001; 
} 

distributed         false;

roots();

preservePatches (hubside1 hubside2);

