FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];


internalField   uniform (8.4 0 0);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           $internalField;
    }
    outlet
    {
        type            zeroGradient;
    }
    ubend
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    ubendup
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    sym
    {
         type           symmetry;
    }
    
}


// ************************************************************************* //
