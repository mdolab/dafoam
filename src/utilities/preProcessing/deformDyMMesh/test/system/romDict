FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      romDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
useDEIM              1;
nDEIMPoints          40;
nSnapshots           100;
nBasisVectors        20;
nBasisVectorsP       10;
snapshotDT           0.02;
romDT                1e-5;
debug                0;
writeASolFreq        1000;
writeObjFuncHist     1;
writeProbeHist       1;
writeFieldHist       1;
probeLocation        (2.5 -0.25 0);
predictEndTime       10;
objFunc
{
    patches     (wing);
    scale       100.0;
    momentAxis  (0 0 1);
    momentCenter (0.2349231552 -0.0855050358 0.05);    
}

