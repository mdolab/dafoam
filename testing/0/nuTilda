FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nuTilda;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0.024;

boundaryField
{
    inlet
    {
        type        fixedValue;
	value	    $internalField;
    }
    outlet
    {
        type        zeroGradient;
    }
   obstacle 
    {
        type        fixedValue;
	value		uniform 0;
    }
        wall
    {
        type        symmetry;

    }

    frontAndBack
    {
        type        symmetry;
    }
}
